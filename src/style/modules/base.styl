/*==============#BASE STYLE================*/
body
	font $font_size $font_segoe_regular

h1,h2,h3
	font-family $font_segoe_semibold
	
h1
	font-size 24px
	
h2
	font-size 18px
	
h3
	font-size 16px
	
ul
	list-style none
	margin 0
	padding 0

.cf:before, .cf:after
	content " "
	display table

.cf:after
	clear both